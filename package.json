{
  "name": "gachchan",
  "version": "7.1.1",
  "description": "Change to TypeScript from v6. Share lib for common helper functions, work in node and browser",
  "releasenote": "Use Parcel to build.",
  "author": "lockevn",
  "license": "LICENSE",
  "keywords": [
    "node",
    "browser",
    "lib",
    "utils",
    "common"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "src/",
    "test/",
    "index.js",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/gachchan.umd.cjs",
  "module": "./dist/gachchan.es.mjs",
  "exports": {
    ".": {
      "require": "./dist/gachchan.umd.cjs",
      "import": "./dist/gachchan.es.mjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "parcel watch",
    "prebuild": "rimraf dist",
    "lint": "eslint . --fix",
    "check": "tsc --noEmit --skipLibCheck",
    "build": "parcel build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "npm run lint && npm run check && npm run test:run && npm run build"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "@parcel/bundler-library": "latest",
    "@parcel/config-default": "latest",
    "@parcel/packager-ts": "latest",
    "@parcel/transformer-typescript-tsc": "latest",
    "@parcel/transformer-typescript-types": "latest",
    "@types/node": "latest",
    "eslint": "latest",
    "parcel": "latest",
    "rimraf": "latest",
    "typescript": "latest",
    "vitest": "latest"
  },
  "peerDependencies": {
    "axios": ">0.21.0",
    "decimal.js": "^10.2.1",
    "lodash": "^4.17.21"
  }
}
